sp {pick-up*elaborate*desired
   (state <s> ^name pick-up
              ^superstate <ss>)
   (<ss>  ^operator <so>)
   (<so> ^name pick-up
         ^moving-block <block>)
-->
   (<s> ^desired <d>)
   (<d> ^moving-block <block>)
}
sp {put-down*elaborate*desired
   (state <s> ^name put-down
              ^superstate <ss>)
   (<ss>  ^operator <so>)
   (<so> ^name put-down
         ^moving-block <block>
         ^destination <dest>)
-->
   (<s> ^desired <d>)
   (<d> ^moving-block <block>
        ^destination <dest>)
}
