sp {blocks-world*elaborate*state*clear-table
   (state <s> ^name blocks-world 
              ^object <object>)
   (<object> ^type table)
-->
   (<s> ^clear <object>)
}

sp {blocks-world*elaborate*state*clear
   (state <s> ^name blocks-world 
              ^object <block>)
  -{(<s> ^ontop.bottom-block <block>)}
-->
   (<s> ^clear <block>)
}