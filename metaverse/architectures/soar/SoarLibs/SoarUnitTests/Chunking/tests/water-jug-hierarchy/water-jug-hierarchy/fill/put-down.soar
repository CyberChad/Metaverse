sp {fill*propose*put-down
   (state <s> ^name fill
              ^jug <j>)
   (<j> ^picked-up yes
        ^filled-jug yes)
-->
   (<s> ^operator <op> + =)
   (<op> ^name put-down)
}
sp {apply*put-down
   (state <s> ^operator <op>
              ^jug <j>)
   (<op> ^name put-down)
   (<j> ^volume <volume>
        ^contents <contents>)
-->
   (<j> ^picked-up yes -
        ^filled-jug yes -
        ^contents <volume>
                  <contents> -)}

