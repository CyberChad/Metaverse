sp {fill*propose*fill-jug
   (state <s> ^name fill
              ^jug <j>)
   (<j> ^picked-up yes
       -^filled-jug yes)
-->
   (<s> ^operator <op> + =)
   (<op> ^name fill-jug)
}

sp {apply*fill-jug
   (state <s> ^operator <op>
              ^jug <j1>)
   (<op> ^name fill-jug)
-->
   (<j1> ^filled-jug yes)
}
